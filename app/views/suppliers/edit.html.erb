<h1>Edit the Supplier:</h1>

<%= form_tag "/suppliers/#{supplier.id}", method: :patch do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :phone %>
    <%= text_field_tag :phone %>
  </div>

  <%= submit_tag "Edit Supplier" %>
<% end %>